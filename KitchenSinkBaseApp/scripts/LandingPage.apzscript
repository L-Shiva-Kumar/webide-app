    if (apz.XACBA.LandingPage.fromDashToAllAccounts === true) {
        if (accType === "CASA") {
            apz.XACBA.LandingPage.interfaceName = "accListCASA";
            apz.XACBA.LandingPage.SERVICETYPE = "CASA";
