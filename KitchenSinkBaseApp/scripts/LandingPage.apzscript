apz.XACBA.LandingPage.startNetworkListenerCallBack = function(params) {
    debugger;
    if (params.event === "off") {
        apz.dispMsg({
            'code': 'APZ_INTERNETERROR'
        });
        flag = true;
    } else {
        if (flag === true) {
            apz.dispMsg({
                'code': 'APZ_INTERNETCONNECTED'
            })
        }
    }
}
