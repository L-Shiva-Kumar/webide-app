        .length; activeArry++) {
        for (var activeArry1 = 0; activeArry1 < Object(apz.XACBA.LandingPage.SPARAMS.dashboardObj.accountSummaryResp.accountDtlsResp
                .acctCcyWiseAry)[activeArry].length; activeArry1++) {
