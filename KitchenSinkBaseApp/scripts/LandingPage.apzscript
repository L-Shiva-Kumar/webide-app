        rFlag = 'P'
    } else if (orn === 'LANDSCAPE') {
        rFlag = 'L'
    }
}
apz.XACBA.LandingPage.setOrientation = function() {
    debugger;
    var rMode;
    if (rFlag === 'P') {
        rMode = "LANDSCAPE";
        rFlag = 'L'
    } else if (rFlag === 'L') {
        rMode = "PORTRAIT";
        rFlag = 'P'
    }
    var json = {};
    json.id = "ORIENTATION";
    json.callBack = apz.XACBA.LandingPage.setOrientationCB;
    json.orientation = rMode;
    apz.ns.setOrientation(json);
}
